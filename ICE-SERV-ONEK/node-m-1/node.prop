# The IceGrid locator proxy.
Ice.Default.Locator=DemoIceGrid/Locator:tcp -h 192.168.3.2 -p 4061:ws -h 192.168.3.2 -p 4062
#节点名
IceGrid.Node.Name=node1
#指定节点1用于客户端连接的端口号
IceGrid.Node.Endpoints=tcp -p 5001
#节点1相关数据的存储目录
IceGrid.Node.Data=./node-db
 
